#define _CRT_SECURE_NO_WARNINGS
#include <stdio.h>

int main() {
	int arr[100];
	int max = -1000, min = 1000;
	for (int i = 0; i < 100; i++) {
		scanf("%d", &arr[i]);
		if (arr[i] == 999) {
			break;
		}
		if (arr[i] > max) {
			max = arr[i];
		}
		if (arr[i] < min) {
			min = arr[i];
		}
		
	}
	printf("max : %d\nmin : %d", max, min);
}
