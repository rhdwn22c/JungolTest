#include <stdio.h>

int main() {
	int arr[2][4];
	int arr2[2][4];

	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < 4; j++) {
			scanf("%d", &arr[i][j]);
		}
	}
	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < 4; j++) {
			scanf("%d", &arr2[i][j]);
		}
	}
	printf("first array\nsecond array\n");
	for (int i = 0; i < 2; i++) {
		for (int j = 0; j < 4; j++) {
			printf("%d ", arr[i][j] * arr2[i][j]);
		}
		printf("\n");
	}
}
